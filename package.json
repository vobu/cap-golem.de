{
  "name": "cap-golem.de",
  "version": "1.0.0",
  "description": "CAP + UI5 = Full Stack SAP",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-beerbrowser": "cds watch --open beerbrowser/webapp/index.html?sap-ui-xx-viewCache=false",
    "start": "cds deploy --to sqlite; cds run"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vobu/ui5-golem.de.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vobu/ui5-golem.de/issues"
  },
  "homepage": "https://github.com/vobu/ui5-golem.de#readme",
  "dependencies": {
    "@sap/cds-graphql": "^1.1.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.6"
  },
  "cds": {
    "plugins": [
      "@sap/cds-graphql",
      "@sap/cds-odata-v2-adapter-proxy"
    ],
    "requires": {
      "db": {
        "kind": "sqlite",
        "credentials": {
          "database": "sqlite.db"
        }
      }
    }
  },
  "devDependencies": {
    "@sap/ux-specification": "^1.102.5",
    "sqlite3": "^5.0.11"
  },
  "sapux": [
    "app/beerbrowser"
  ]
}